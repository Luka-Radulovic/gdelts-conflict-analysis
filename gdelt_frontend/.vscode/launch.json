{
    "version": "0.2.0",
    "configurations": [
      {
        "command": "npm start",
        "name": "Run npm start",
        "request": "launch",
        "type": "node-terminal"
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome to Debug",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true,
        "skipFiles": [
          "<node_internals>/**",
          "${workspaceFolder}/node_modules/**",
          "${workspaceFolder}/index.html",
          "**/leaflet.js"
        ]
      }
    ],
    "compounds": [
      {
        "name": "Debug GDELT frontend",
        "configurations": ["Launch Chrome to Debug","Run npm start"]
      }
    ]
  }
  